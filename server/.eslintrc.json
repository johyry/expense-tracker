{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "semi": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": 0
  }
}
